                    <select id="ddl_Rules" multiple="multiple" class="form-control"></select>
                </div>
                <div class="form-group Designation">
                    <label for="DesignationReport">Designation Report</label>
                    <select id="ddl_DesignationReport" class="form-control" multiple="multiple"></select>
                </div>

                <div class="form-group">
                    <label for="Grade">Grade</label>
                    <select id="ddl_Grade" class="form-control">
