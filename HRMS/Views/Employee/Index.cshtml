            <div class="col-md-12" align="center">
                <button type="button" class="btn btn-primary" id="btnAdd" onclick="return FinalAdd();">Add</button>
                <button type="button" class="btn btn-primary" id="btnNext" onclick="return Previous2();">Previous</button>
                <button type="button" class="btn btn-primary" id="btnForword" onclick="return Forword();">Next</button>
                <button type="button" class="btn btn-primary" onclick="clearTextBox();">Close</button>
            </div>
