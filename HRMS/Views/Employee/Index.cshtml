                        <option>B+</option>
                        <option>B-</option>
                        <option>O+</option>
                        <option>O-</option>
                        <option>AB+</option>
                        <option>AB-</option>
                    </select>
                </div>
                <div class="form-group">
                    <label for="Experience">
                        Type Experience
                    </label>
                    <select id="ddl_Experience" class="form-control">
                        <option value="0">Select</option>
                        <option value="1">Fresher</option>
                        <option value="2">Experience</option>
                    </select>
                </div>
                <div id="PreviousEx1">
                    <div class="form-group">
                        <label for="Previous Designation">
                            Previous Designation
                        </label>
                        <input type="text" class="form-control" id="txt_PreviousDeg" placeholder="Previous Designation" maxlength="16" />
                    </div>
                    <div class="form-group">
                        <label for="Salary/CTC">
                            Salary/CTC

                        </label>
                        <input type="text" class="form-control" id="txt_SalaryCTC" placeholder="Salary/CTC" maxlength="16" />
                    </div>
                </div>
            </div>
            <div class="col-md-12" align="center">
                <button type="button" class="btn btn-primary" id="btnAdd" onclick="AddShift();">Add</button>
                <button type="button" class="btn btn-primary" id="btnUpdate" onclick="UpdateShift();" style="display:none">Update</button>
                <button type="button" class="btn btn-primary" onclick="clearTextBox();">Clear</button>
            </div>
        </div>

        <div id="Secound" class="row" style="display:none;">
            <div class="col-md-6">
                <div class="form-group">
                    <label for="group">Group</label>
                    <select id="ddl_Group" class="form-control">
                        <option value="0">Select</option>
                    </select>

                </div>
