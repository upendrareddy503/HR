                <div class="form-group">
                    <label for="PhoneNo">PhoneNo</label>
                    <input type="text" class="form-control" id="txt_PhoneNo" placeholder="PhoneNo" maxlength="12" onkeypress="return isNumberKey(event)" />
                    <span class="error_show form-text has-danger" id="spPhoneValid">Please Enter Valid Phone!.</span>
                </div>
                <div class="form-group">
                    <label for="Dateofjoining">Date of joining</label>
                    <input type="text" id="txt_DOJ" class="form-control datepicker" placeholder="01/01/1999" maxlength="10">
