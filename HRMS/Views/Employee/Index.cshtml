                    <select id="ddl_Designation" class="form-control">
                        <option value="0">Select</option>
                    </select>
                </div>
                <div class="form-group">
                    <label for="WeekOff">Week Off</label>
                    <select id="ddl_WeekOff" class="form-control">
                        <option value="0">Select</option>
                        <option value="1">Sunday</option>
