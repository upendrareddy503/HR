

<script src="~/JS/Company.js"></script>



<!-- Page Content -->
<div class="content container-fluid">
    <!-- Page Header -->
    <div class="page-header">
        <div class="row align-items-center">
            <div class="col">
                <h3 class="page-title">Group</h3>
                <ul class="breadcrumb">
                    <li class="breadcrumb-item"><a href="index-2.html">Dashboard</a></li>
                    <li class="breadcrumb-item active">Group</li>
                </ul>
            </div>
            <div class="col-auto float-right ml-auto">
                <a href="#" class="btn add-btn" data-toggle="modal" data-target="#add_Company"><i class="fa fa-plus"></i> Add Company</a>
            </div>
        </div>
    </div>
    <!-- /Page Header -->
    <div class="row">
        <div class="col-md-12">
            <div class="card mb-0">

                <div class="card-body">
                    <div class="table-responsive">
                        <table id="tblCompany" class="table table-stripped mb-0">
                            <thead>
                                <tr>
                                    @*<th style="width: 30px;">#</th>*@

                                    <th>Sno</th>
                                    <th>Company Name</th>
                                    <th class="text-right">Action</th>
                                </tr>
                            </thead>
                            <tbody></tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>



</div>
<!-- /Page Content -->

<div id="add_Company" class="modal custom-modal fade" role="dialog">
    <div class="modal-dialog modal-dialog-centered modal-lg" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title">Add Company Master</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <form>
                    <div class="row">
                        
                        <div class="col-sm-6">

                            <div class="form-group">
                                <label for="CompanyName">Company Name</label>
                                <input type="text" class="form-control" id="txt_CompanyName" placeholder="Company Name" />
                            </div>

                            <div class="form-group">
                                <label for="Code">Code</label>
                                <input type="text" class="form-control" id="txt_Code" placeholder="Code" />
                            </div>

                            <div class="form-group">
                                <label for="Address">Address</label>
                                <input type="text" class="form-control" id="txt_Address" placeholder="Address" />
                            </div>

                            <div class="form-group">
                                <label for="City">City</label>
                                <input type="text" class="form-control" id="txt_City" placeholder="City" />
                            </div>

                            <div class="form-group">
                                <label for="District">District</label>
                                <input type="text" class="form-control" id="txt_District" placeholder="District" />
                            </div>

                            <div class="form-group">
                                <label for="State">State</label>
                                <select id="ddl_State" class="select form-control"><option>Select</option></select>
                                @*<input type="text" class="form-control" id="txt_State" placeholder="State" />*@
                            </div>

                            <div class="form-group">
                                <label for="Country">Country</label>
                                <select id="ddl_Country" class="select form-control"><option>Select</option></select>
                                @*<input type="text" class="form-control" id="txt_Country" placeholder="Country" />*@
                            </div>
                        </div>
                        <div class="col-sm-6">
                          
                            <div class="form-group">
                                <label for="PhoneNo">PhoneNo</label>
                                <input type="text" class="form-control" id="txt_PhoneNo" placeholder="PhoneNo" />
                            </div>
                            <div class="form-group">
                                <label for="EstDate">EstDate</label>

                                <input type="text" id="datepicker" class="form-control" placeholder="01/01/1999">

                            </div>
                            <div class="form-group">
                                <label for="EmailID">EmailID</label>
                                <input type="text" class="form-control" id="txt_EmailID" placeholder="EmailID" />
                            </div>
                            <div class="form-group">
                                <label for="GST">GST</label>
                                <input type="text" class="form-control" id="txt_GST" placeholder="GST" />
                            </div>
                            <div class="form-group">
                                <label for="Tan">Tan Number</label>
                                <input type="text" class="form-control" id="txt_Tan" placeholder="Tan Number" />
                            </div>

                            <div class="form-group">
                                <label for="Pan">Pan No</label>
                                <input type="text" class="form-control" id="Pan" placeholder="Pan Number" />
                            </div>
                            <div class="form-group">
                                <label for="Website">Website</label>
                                <input type="text" class="form-control" id="txt_Website" placeholder="Website" />
                            </div>

                        </div>

                    </div>
                    <div class="submit-section">
                        <button type="button" class="btn btn-primary" id="btnAdd" onclick="return Add();">Add</button>
                        <button type="button" class="btn btn-primary" id="btnUpdate" onclick="Update();" style="display:none">Update</button>
                        <button type="button" class="btn btn-default" onclick="clearTextBox();">Close</button>
                    </div>
                </form>
            </div>
        </div>
    </div>
</div>




